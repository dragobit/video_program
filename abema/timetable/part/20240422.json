                "tableStartAt": 1713713400,
                "tableEndAt": 1713715200,
                "highlight": "じいさんばあさん若返る #3",
                "displayProgramId": "175-1rthzhecdme_s0_p35",
                "mark": {
                    "newcomer": true,
                    "recommendation": true
                },
                "flags": {
                    "timeshift": true,
