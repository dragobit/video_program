                "tableStartAt": 1713723480,
                "tableEndAt": 1713726420,
                "highlight": "劇場版あたしンち",
                "displayProgramId": "35-52_s0_p1",
                "mark": {},
                "flags": {
                    "timeshift": true,
                    "timeshiftFree": true,
