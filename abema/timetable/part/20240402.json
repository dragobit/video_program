                "endAt": 1711989000,
                "tableStartAt": 1711987200,
                "tableEndAt": 1711989000,
                "highlight": "第七王子 #1",
                "displayProgramId": "175-1rthzhecdme_s0_p93",
                "mark": {
                    "newcomer": true,
                    "recommendation": true
                },
                "flags": {
                    "timeshift": true,
                    "timeshiftFree": true,
                    "chasePlay": true,
