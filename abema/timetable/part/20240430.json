                "endAt": 1714406400,
                "tableStartAt": 1714404600,
                "tableEndAt": 1714406400,
                "highlight": "デート・ア・ライブⅤ #3",
                "displayProgramId": "175-1rthzhecdme_s0_p16",
                "mark": {
                    "newcomer": true,
                    "recommendation": true
                },
                "flags": {
                    "timeshift": true,
