                "tableStartAt": 1713110400,
                "tableEndAt": 1713112200,
                "highlight": "ブルーアーカイブ The Animation #2",
                "displayProgramId": "175-1rthzhecdme_s0_p71",
                "mark": {
                    "newcomer": true,
                    "recommendation": true
                },
                "flags": {
                    "timeshift": true,
